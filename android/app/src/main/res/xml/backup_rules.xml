<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample backup rules file; uncomment and customize as needed.
   See https://developer.android.com/guide/topics/data/autobackup
   for more information.
-->
<full-backup-content>
    <!-- Exclude sensitive authentication data -->
    <exclude domain="sharedpref" path="auth_prefs.xml"/>
    <exclude domain="database" path="vocalysis.db"/>
    <!-- Include user preferences -->
    <include domain="sharedpref" path="user_prefs.xml"/>
</full-backup-content>
